#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/@sveltejs+kit@1.0.0-next.123_svelte@3.38.3/node_modules/@sveltejs/kit/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/@sveltejs+kit@1.0.0-next.123_svelte@3.38.3/node_modules/@sveltejs/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/@sveltejs+kit@1.0.0-next.123_svelte@3.38.3/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules:/home/aaron/sandbox/svelte/node_modules:/home/aaron/sandbox/node_modules:/home/aaron/node_modules:/home/node_modules:/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/@sveltejs/kit/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/@sveltejs/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/@sveltejs+kit@1.0.0-next.123_svelte@3.38.3/node_modules/@sveltejs/kit/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/@sveltejs+kit@1.0.0-next.123_svelte@3.38.3/node_modules/@sveltejs/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/@sveltejs+kit@1.0.0-next.123_svelte@3.38.3/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules:/home/aaron/sandbox/svelte/node_modules:/home/aaron/sandbox/node_modules:/home/aaron/node_modules:/home/node_modules:/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/@sveltejs/kit/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/@sveltejs/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@sveltejs/kit/svelte-kit.js" "$@"
else
  exec node  "$basedir/../@sveltejs/kit/svelte-kit.js" "$@"
fi
