#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/svgo@2.3.1/node_modules/svgo/bin/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/svgo@2.3.1/node_modules/svgo/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/svgo@2.3.1/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules:/home/aaron/sandbox/svelte/node_modules:/home/aaron/sandbox/node_modules:/home/aaron/node_modules:/home/node_modules:/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/postcss-svgo@5.0.2_postcss@8.3.5/node_modules/postcss-svgo/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/postcss-svgo@5.0.2_postcss@8.3.5/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/svgo@2.3.1/node_modules/svgo/bin/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/svgo@2.3.1/node_modules/svgo/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/svgo@2.3.1/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules:/home/aaron/sandbox/svelte/node_modules:/home/aaron/sandbox/node_modules:/home/aaron/node_modules:/home/node_modules:/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/postcss-svgo@5.0.2_postcss@8.3.5/node_modules/postcss-svgo/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/postcss-svgo@5.0.2_postcss@8.3.5/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../svgo@2.3.1/node_modules/svgo/bin/svgo" "$@"
else
  exec node  "$basedir/../../../../../svgo@2.3.1/node_modules/svgo/bin/svgo" "$@"
fi
