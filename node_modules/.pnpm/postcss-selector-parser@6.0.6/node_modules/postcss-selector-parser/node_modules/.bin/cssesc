#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/cssesc@3.0.0/node_modules/cssesc/bin/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/cssesc@3.0.0/node_modules/cssesc/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/cssesc@3.0.0/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules:/home/aaron/sandbox/svelte/node_modules:/home/aaron/sandbox/node_modules:/home/aaron/node_modules:/home/node_modules:/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/postcss-selector-parser@6.0.6/node_modules/postcss-selector-parser/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/postcss-selector-parser@6.0.6/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/cssesc@3.0.0/node_modules/cssesc/bin/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/cssesc@3.0.0/node_modules/cssesc/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/cssesc@3.0.0/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules:/home/aaron/sandbox/svelte/node_modules:/home/aaron/sandbox/node_modules:/home/aaron/node_modules:/home/node_modules:/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/postcss-selector-parser@6.0.6/node_modules/postcss-selector-parser/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/postcss-selector-parser@6.0.6/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../cssesc@3.0.0/node_modules/cssesc/bin/cssesc" "$@"
else
  exec node  "$basedir/../../../../../cssesc@3.0.0/node_modules/cssesc/bin/cssesc" "$@"
fi
