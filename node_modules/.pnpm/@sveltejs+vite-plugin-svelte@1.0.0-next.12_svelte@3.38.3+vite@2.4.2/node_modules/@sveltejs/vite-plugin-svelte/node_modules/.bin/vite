#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/vite@2.4.2/node_modules/vite/bin/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/vite@2.4.2/node_modules/vite/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/vite@2.4.2/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules:/home/aaron/sandbox/svelte/node_modules:/home/aaron/sandbox/node_modules:/home/aaron/node_modules:/home/node_modules:/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/@sveltejs+vite-plugin-svelte@1.0.0-next.12_svelte@3.38.3+vite@2.4.2/node_modules/@sveltejs/vite-plugin-svelte/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/@sveltejs+vite-plugin-svelte@1.0.0-next.12_svelte@3.38.3+vite@2.4.2/node_modules/@sveltejs/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/@sveltejs+vite-plugin-svelte@1.0.0-next.12_svelte@3.38.3+vite@2.4.2/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/vite@2.4.2/node_modules/vite/bin/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/vite@2.4.2/node_modules/vite/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/vite@2.4.2/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules:/home/aaron/sandbox/svelte/node_modules:/home/aaron/sandbox/node_modules:/home/aaron/node_modules:/home/node_modules:/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/@sveltejs+vite-plugin-svelte@1.0.0-next.12_svelte@3.38.3+vite@2.4.2/node_modules/@sveltejs/vite-plugin-svelte/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/@sveltejs+vite-plugin-svelte@1.0.0-next.12_svelte@3.38.3+vite@2.4.2/node_modules/@sveltejs/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/@sveltejs+vite-plugin-svelte@1.0.0-next.12_svelte@3.38.3+vite@2.4.2/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vite@2.4.2/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../../vite@2.4.2/node_modules/vite/bin/vite.js" "$@"
fi
