#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/eslint@7.30.0/node_modules/eslint/bin/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/eslint@7.30.0/node_modules/eslint/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/eslint@7.30.0/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules:/home/aaron/sandbox/svelte/node_modules:/home/aaron/sandbox/node_modules:/home/aaron/node_modules:/home/node_modules:/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/eslint-plugin-svelte3@3.2.0_eslint@7.30.0+svelte@3.38.3/node_modules/eslint-plugin-svelte3/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/eslint-plugin-svelte3@3.2.0_eslint@7.30.0+svelte@3.38.3/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/eslint@7.30.0/node_modules/eslint/bin/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/eslint@7.30.0/node_modules/eslint/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/eslint@7.30.0/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules:/home/aaron/sandbox/svelte/node_modules:/home/aaron/sandbox/node_modules:/home/aaron/node_modules:/home/node_modules:/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/eslint-plugin-svelte3@3.2.0_eslint@7.30.0+svelte@3.38.3/node_modules/eslint-plugin-svelte3/node_modules:/home/aaron/sandbox/svelte/svelte-markdown-blog/node_modules/.pnpm/eslint-plugin-svelte3@3.2.0_eslint@7.30.0+svelte@3.38.3/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../eslint@7.30.0/node_modules/eslint/bin/eslint.js" "$@"
else
  exec node  "$basedir/../../../../../eslint@7.30.0/node_modules/eslint/bin/eslint.js" "$@"
fi
